<%- include('../partials/header') %>
<div class="post-cards">
<% for (let cohort of cohorts) { %>
    <div class="card mb-4">
      <div class="card-body">
        <div class="card-title">
          <img class="small-image" src="<%= cohort.imageURL %>">
          <h2><%= cohort.name %></h2>
        </div>
        <p class="card-text"><%= cohort.members %></p>
        <a class="btn btn-primary" href="/cohorts/<%= cohort.id %>" role="button">Show Cohort</a> 
      </div>
    </div>
</div>
<% } %>
<%- include('../partials/footer') %>