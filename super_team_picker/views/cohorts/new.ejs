<%- include('../partials/header') %>
<div class="form-spacing">
<form action="/cohorts" method="POST">
  <h4>Logo Url</h4>
    <input type="text" name="imageURL" class="form-control" placeholder="e.g. https://www.example.com/some.png">
  <h4>Name</h4>
      <input type="text" name="name" class="form-control" placeholder="cohort name...">
  <h4>Members</h4>
      <textarea class="form-control" name="members" placeholder="names of members..."></textarea>
    <div class="button-container">
    <input type="submit" class="btn btn-outline-primary" value="Submit">
    </div>
</form>
</div>
<%- include('../partials/footer') %>