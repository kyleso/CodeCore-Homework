{
  "name": "super_team_picker",
  "version": "0.0.1",
  "description": "express.js app with database for picking teams of various sizes in a cohort",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "createdb --echo super_team_picker",
    "db:drop": "dropdb --if-exists --echo super_team_picker",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate"
  },
  "author": "Kyle So",
  "license": "ISC",
  "dependencies": {
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "knex": "^0.16.3",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "pg": "^7.8.0"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "nodemon": "^1.18.10"
  }
}
